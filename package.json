{
  "name": "gabum",
  "version": "0.0.0",
  "description": "Refactoring of Gabum (Project Manager) with NodeJS ",
  "main": "index.js",
  "repository": "git@github.com:Galitan-dev/Gabum-V2.git",
  "author": "Galitan-dev <galitan.dev@gmail.com>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.9.0",
    "husky": "^7.0.4",
    "pinst": "^3.0.0",
    "standard-version": "^9.3.2"
  },
  "scripts": {
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "lint": "eslint",
    "release": "standard-version",
    "postpublish": "git push --follow-tags origin main"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        2,
        "never",
        [
          "start-case",
          "pascal-case"
        ]
      ]
    }
  }
}
