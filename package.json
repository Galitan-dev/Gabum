{
  "name": "gabum",
  "description": "A simple project manager made with Node JS.",
  "version": "1.7.2",
  "author": "Galitan-dev",
  "bin": {
    "gabum": "./bin/run"
  },
  "bugs": "https://github.com/oclif/hello-world/issues",
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5.1.11",
    "@oclif/plugin-not-found": "^2.3.1",
    "@oclif/plugin-which": "^2.1.0",
    "adm-zip": "^0.5.9",
    "detect-package-manager": "^2.0.1",
    "enquirer": "^2.3.6",
    "figlet": "^1.5.2",
    "listr2": "^4.0.5",
    "prompts": "^2.4.2",
    "rxjs": "^7.5.5",
    "shelljs": "^0.8.5",
    "superagent": "^7.1.1",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.4.34",
    "@types/figlet": "^1.5.4",
    "@types/prompts": "^2.0.14",
    "@types/shelljs": "^0.8.11",
    "@types/superagent": "^4.1.15",
    "@types/yaml": "^1.9.7",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "globby": "^11",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "oclif": "^2",
    "prettier": "^2.5.1",
    "shx": "^0.3.4",
    "ts-node": "^10.7.0",
    "tsc": "^2.0.4",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/galitan-dev/gabum",
  "keywords": [
    "oclif",
    "project manager",
    "cli",
    "ts"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "bin": "gabum",
    "dirname": "gabum",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-which"
    ],
    "topicSeparator": " ",
    "topics": {
      "project": {
        "description": "Project Management"
      }
    },
    "hooks": {
      "init": [
        "./dist/hooks/init/cli_requirements",
        "./dist/hooks/init/config"
      ]
    }
  },
  "repository": "https://github.com/Galitan-dev/Gabum",
  "scripts": {
    "build": "shx rm -rf dist && mkdir -p ./dist/res && cp ./src/res/* ./dist/res && tsc -b",
    "dev": "yarn build && ./bin/dev",
    "gabum": "./bin/run",
    "lint": "eslint . --ext .ts --config .eslintrc.json",
    "lint:fix": "eslint . --fix --ext .ts --config .eslintrc.json",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "yarn build && oclif manifest && oclif readme --multi && git add README.md docs",
    "version": "oclif readme --multi && git add README.md docs"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "yarn run lint:fix"
  },
  "types": "dist/index.d.ts",
  "private": false,
  "resolutions": {
    "graceful-fs": "^4.2.9"
  }
}
