{
  "name": "gabum",
  "description": "A simple project manager made with Node JS.",
  "version": "0.1.6",
  "author": "Galitan-dev",
  "bin": {
    "gabum": "./bin/run"
  },
  "bugs": "https://github.com/oclif/hello-world/issues",
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-commands": "^2.1.0",
    "@oclif/plugin-help": "^5.1.11",
    "@oclif/plugin-not-found": "^2.3.1",
    "@oclif/plugin-which": "^2.1.0",
    "conf-cli": "^0.1.9",
    "oclif-plugin-completion": "^0.6.0",
    "prompts": "^2.4.2",
    "spdx-license-list": "^6.4.0"
  },
  "devDependencies": {
    "@types/node": "^16.9.4",
    "@types/prompts": "^2.0.14",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "eslint": "^7.32.0",
    "globby": "^11",
    "oclif": "^2",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/oclif/hello-world",
  "keywords": [
    "oclif",
    "project manager",
    "cli",
    "ts"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "bin": "gabum",
    "dirname": "gabum",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-which",
      "@oclif/plugin-commands",
      "oclif-plugin-completion",
      "conf-cli"
    ],
    "topicSeparator": " ",
    "topics": {
      "project": {
        "description": "Project Management"
      }
    }
  },
  "repository": "https://github.com/Galitan-dev/Gabum",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "dev": "yarn build && ./bin/dev",
    "lint": "eslint . --ext .ts --config .eslintrc.yml",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "version": "oclif readme --multi && git add README.md"
  },
  "types": "dist/index.d.ts",
  "private": false
}
